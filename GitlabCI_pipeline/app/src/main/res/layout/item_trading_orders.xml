<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/volumeBuyValueTextView"
        style="@style/TextAppearance.Medium.10.Tundora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        app:layout_constraintEnd_toStartOf="@+id/oneQuarterGuideline"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="0.2" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/priceBuyValueTextView"
        style="@style/TextAppearance.Medium.10.Tundora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:textColor="@color/shamrock"
        app:layout_constraintEnd_toStartOf="@+id/aHalfGuideline"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/oneQuarterGuideline"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="29710.99" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/priceSellValueTextView"
        style="@style/TextAppearance.Medium.10.Tundora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:textColor="@color/bittersweet"
        app:layout_constraintEnd_toStartOf="@+id/threeQuartersGuideline"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/aHalfGuideline"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="29710.99" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/volumeSellValueTextView"
        style="@style/TextAppearance.Medium.10.Tundora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/threeQuartersGuideline"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="0.056412" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/threeQuartersGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/aHalfGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/oneQuarterGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

</androidx.constraintlayout.widget.ConstraintLayout>